{% extends 'base.html.twig' %}

{% block title %}
    Show report {{ report.id|default('') }}
{% endblock %}

{% block body %}
    <h1>View report {{ report.id|default('') }}</h1>
    {% if report is defined and report|length %}
    <dl class="dl-horizontal">
        <dt>Id</dt>
        <dd>{{ report.id }}</dd>
        <dt>Title</dt>
        <dd>{{ report.title }}</dd>
        <dt>Created at</dt>
        <dd>{{ report.createdAt }}</dd>
        <dt>Updated at</dt>
        <dd>{{ report.updatedAt }}</dd>
    </dl>
    {% else %}
    <p>Report not found.</p>
    {% endif %}
{% endblock %}